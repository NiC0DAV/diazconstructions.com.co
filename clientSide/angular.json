{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "clientSide": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/client-side",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "src/assets/css/style.css",
              "src/assets/css/bootstrap.css",
              "src/assets/css/revolution-slider.css",
              "src/assets/css/error-page.css",
              "src/assets/css/error-page-responsive.css",
              "src/assets/css/responsive.css"
            ],
            "scripts": [
              "src/assets/js/jquery.js",
              // "node_modules/jquery/dist/jquery.min.js",
              "src/assets/js/revolution.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery.mCustomScrollbar.concat.min.js",
              "src/assets/js/jquery.fancybox.pack.js",
              "src/assets/js/jquery.fancybox-media.js",
              "src/assets/js/isotope.js",
              "src/assets/js/owl.js",
              "src/assets/js/wow.js",
              "src/assets/js/jquery.parallaxmouse.min.js",
              "src/assets/js/script.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "clientSide:build:production"
            },
            "development": {
              "browserTarget": "clientSide:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "clientSide:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "src/assets/css/style.css",
              "src/assets/css/bootstrap.css",
              "src/assets/css/revolution-slider.css",
              "src/assets/css/error-page.css",
              "src/assets/css/error-page-responsive.css",
              "src/assets/css/responsive.css"
            ],
            "scripts": [
              "src/assets/js/jquery.js",
              // "node_modules/jquery/dist/jquery.min.js",
              "src/assets/js/revolution.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery.mCustomScrollbar.concat.min.js",
              "src/assets/js/jquery.fancybox.pack.js",
              "src/assets/js/jquery.fancybox-media.js",
              "src/assets/js/isotope.js",
              "src/assets/js/owl.js",
              "src/assets/js/wow.js",
              "src/assets/js/jquery.parallaxmouse.min.js",
              "src/assets/js/script.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
